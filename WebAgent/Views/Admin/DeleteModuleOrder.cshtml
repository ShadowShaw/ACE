@model Core.Models.ModuleOrder

@{
    ViewBag.Title = "Smazat objednávku modulu";
}

@Html.Partial("PageTitlePartial")

<h3>Jste si jisti, zda chce opravdu smazat objednávku?</h3>
<fieldset>
    <div class="display-label">
         @Html.DisplayNameFor(model => model.UserId)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UserId)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.ModuleId)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ModuleId)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.OrderDate)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.OrderDate)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" class="btn btn-primary" value="DeleteModuleOrder" /> 
        @Html.ActionLink("Zpět na seznam objednávek modulů", "ModuleOrders", routeValues: null, htmlAttributes: new { @class="btn btn-default" })
    </p>
}
